public int[] make2(int[] a, int[] b) {
  int[] temparray = new int[2];
    
    if(a.length >= 2){
	temparray[0] = a[0];
	temparray[1] = a[1];
    }
    
    if(a.length == 1){
	temparray[0] = a[0];
	temparray[1] = b[0];
    }
    if(a.length == 0){
	temparray[0] = b[0];
	temparray[1] = b[1];
    }
    return temparray;
}
