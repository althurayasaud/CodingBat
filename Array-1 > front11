public int[] front11(int[] a, int[] b) {
  int[] temparray ;
    if(a.length >=1 && b.length >=1){
        temparray = new int[2];
	temparray[0] = a[0];
	temparray[1] = b[0];
    }
    else if(a.length >=1 && b.length == 0){
	temparray = new int[1];
	temparray[0] = a[0];
    }
    else if(a.length ==0  && b.length >= 1){
	temparray = new int[1];
	temparray[0] = b[0];
    }
    else
	temparray = new int[0];
    
    return temparray;
}
