public String withoutX2(String str) {
  int b=0;
  if(str.length() > 0 && str.charAt(0) == 'x') {
    b=1;
    if(str.length() > 1 && str.charAt(1) == 'x') {
      b=2;
      return str.substring(b);    
    }
    return str.substring(b);
  }
  if(str.length() > 1 && str.charAt(1) == 'x') {
    return str.substring(0,1) + str.substring(2);
  }
  return str;
  
}
